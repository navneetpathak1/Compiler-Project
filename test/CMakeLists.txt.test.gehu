cmake_minimum_required(VERSION 3.10)
project(test_gehu)

# Set C++ standard
set(CMAKE_CXX_STANDARD 17)
set(CMAKE_CXX_STANDARD_REQUIRED ON)

# Add executable
add_executable(test_gehu test.gehu)

# Set compiler options
target_compile_options(test_gehu PRIVATE 
    -Wall 
    -Wextra 
    -Wpedantic
) 